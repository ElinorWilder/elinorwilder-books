        function setPostHogSessionId() {
            setTimeout(() => {
                const sessionId = posthog.get_session_id();
                if (sessionId) {
                    axios.post('https://dashboard.mailerlite.com/api/posthog/session', {
                        session_id: sessionId,
                    });
                }
            }, 3000);
        }

